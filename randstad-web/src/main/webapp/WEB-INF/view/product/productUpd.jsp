<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@include file="../common/common.jsp"%> 
<!-- Content Header (Page header) -->
<!-- Main content -->
<section class="content-header">
<h1>
        General Form Elements
        <small>Preview</small>
      </h1>
</section>
<section class="content">
	<!-- Horizontal Form -->
	<div class="row">
		<div class="col-md-12">
          <div class="box box-info">
            <div class="box-header with-border">
              <h3 class="box-title">Horizontal Form</h3>
            </div>

            <!-- /.box-header -->
            <!-- form start -->
            <form class="form-horizontal">
              <div class="box-body">
              	<div class="form-group">
                  <label for="inputEmail3" class="col-sm-4 control-label">ID</label>
                  <div class="col-sm-4">
                    <input name="id" class="form-control" id="inputEmail3" ng-model="product.id">
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-sm-4 control-label">产品编码</label>
                  <div class="col-sm-4">
                    <input name="proCode" class="form-control" id="inputEmail3" ng-model="product.proCode">
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-sm-4 control-label">产品名称</label>
                  <div class="col-sm-4">
                    <input name="proName" class="form-control" id="inputEmail3" ng-model="product.proName">
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-sm-4 control-label">产品规格</label>
                  <div class="col-sm-4">
                    <input name="proSpec" class="form-control" id="inputEmail3" ng-model="product.proSpec">
                  </div>
                </div>
                <div class="form-group">
                  <label for="inputEmail3" class="col-sm-4 control-label">产品附件A</label>
                  <div class="col-sm-4">
                    <input name="proFile" class="form-control" id="inputEmail3" ng-model="product.proFile">
                  </div>
                </div>

                <div class="form-group">
                  <label for="inputEmail3" class="col-sm-4 control-label">产品owner</label>
                  <div class="col-sm-4">
                    <input name="proOwner" class="form-control" id="inputEmail3" ng-model="product.proOwner">
                  </div>
                </div>
              </div>
              <br><br>
              <!-- /.box-body -->
              <div class="box-footer">
              <div class="col-sm-5">
              <!-- /. -->
              </div>
              <div class="col-sm-2">
                <button type="submit" class="btn btn-default pull-left">Cancel</button>
                <button type="submit" class="btn btn-info pull-right" ng-click="updateProduct()">submit</button>
                </div>
              </div>
              <!-- /.box-footer -->
            </form>
          </div>
		</div>
	</div>
</section>

